<h1> Log In </h1>
<p style="color: red;"><%= flash[:alert] %></p> 
<p><%= flash[:notice] %></p> 
<%= form_tag log_in_path do %>
<%= label_tag :email %>
<br>
<%= text_field_tag :email %>
<br>
<%= label_tag :password %>
<br>
<%= password_field_tag :password %>
<br>
<%= submit_tag "Log in" %>
<% end %>
<br>
<p>Don't have an account yet? <%= link_to("Sign up", sign_up_path) %> for one.</p>
